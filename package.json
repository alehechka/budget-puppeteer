{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint src test",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "pregit": "git add *",
    "git": "git commit",
    "postgit": "git push"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src test"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "budget-puppeteer",
  "author": "Adam Lehechka",
  "module": "dist/budget-puppeteer.esm.js",
  "size-limit": [
    {
      "path": "dist/budget-puppeteer.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/budget-puppeteer.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/dotenv": "^8.2.0",
    "@types/puppeteer": "^5.4.2",
    "husky": "^4.3.6",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "puppeteer": "^5.5.0"
  }
}
